<%= cell_title(@target) %>

<%= cell_content do %>
  <dl>
    <% @traits.each do |i| %>
      <dt><%= i.name %></dt>
      <dd><%= format_trait(@target, i) %></dd>
    <% end %>
  </dl>
<% end %>

<% if @placement.feature.page %>
  <%= link_to page_path(@placement.feature.page), :class => 'btn' do %>
    <i class="icon-pencil"></i>
    Edit
  <% end %>
<% end %>
