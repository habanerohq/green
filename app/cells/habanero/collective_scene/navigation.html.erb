<%= cell_content do %>
  <div class="subnav">
    <ul class="nav nav-pills">
      <% @targets.each do |p| %>
        <li>
          <%= 
            begin 
              link_to p, scene_path(p), :class => :btn
            rescue ActionController::RoutingError
              link_to p, :anchor => '', :class => :btn
            end
          %>
        </li>
      <% end %>
    </ul>
  </div>
<% end %>
