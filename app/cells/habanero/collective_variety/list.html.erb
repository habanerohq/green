<%= cell_title(collection_title) %>

<%= cell_content do %>
  <ul class="unstyled">
    <% @targets.each do |t| %>
      <li>
        <% if @feature.scene %>
          <%= link_to t, scene_path(@feature.scene, list_link(t)) %>
        <% else %>
          <%= t %>
        <% end %>
      </li>
    <% end %>
  </ul>
<% end %>
