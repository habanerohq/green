<%= cell_div do %>
  <%= cell_title(@target) %>

  <%= cell_content do %>
    <dl>
      <% @placement.scoop.mask.mask_ingredients.each do |mi| %>
        <dt><%= mi.ingredient.name %></dt>
        <dd><%= format_ingredient(@target, mi.ingredient) %></dd>
      <% end %>
    </dl>
  <% end %>
<% end %>
